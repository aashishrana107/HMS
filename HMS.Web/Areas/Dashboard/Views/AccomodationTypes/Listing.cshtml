@model HMS.Web.Areas.Dashboard.Models.AccomodationTypesListingModels

@if (Model != null)
{
    <table class="table table-striped">
        <thead>
            <tr>
               
                <th scope="col">Name</th>
                <th scope="col">Description</th>
                <th scope="col">Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var accomodationTypes in Model.AccomodationTypes)
            {


            <tr>
                
                <td>@accomodationTypes.Name</td>
                <td>@accomodationTypes.Description</td>
                <td>
                    <button type="button" class="btn btn-primary"><i class="far fa-edit mr-1"></i> Edit</button>
                    <button type="button" class="btn btn-danger"><i class="far fa-trash-alt mr-1"></i> Delete</button>

                </td>
            </tr>
            }
            
        </tbody>
    </table>
}
else
{
    <div class="alert-danger">
        <text>Record Not Exist.</text>
    </div>
}



